<!-- insert_bottom '.panel-body' -->
<div class="form-group">
  <%= form.label :tax_document_type %>
  <%= form.select :tax_document_type,
                  Spree::Order.tax_document_types.map { |(name)| [Spree.t(name), name] },
                  {},
                  class: 'form-control' %>
</div>
<div class="form-group">
  <%= form.label :tax_document %>
  <%= form.text_field :tax_document, required: true, class: 'form-control required' %>
</div>